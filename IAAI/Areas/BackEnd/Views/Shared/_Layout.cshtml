    <!-- Custom js -->
    <script type="text/javascript" src="~/BackEndTemplate/assets/js/script.js"></script>
    <script src="~/BackEndTemplate/assets/js/pcoded.min.js"></script>
    <script src="~/BackEndTemplate/assets/js/demo-12.js"></script>
    <script src="~/BackEndTemplate/assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
    @*@Scripts.Render("~/bundles/jquery")*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
