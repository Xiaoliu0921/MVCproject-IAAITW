@model IAAI.Models.News

    
<div class="post-content">

    <div class="post-body">
        <div class="entry-header">
            <span class="post-meta-date"><i class="fa fa fa-clock-o"></i> 2016-03-28</span>
            <h2 class="entry-title">
                @Model.Title
            </h2>
        </div><!-- header end -->

        <div class="post-media post-image">
            <img src="@Model.ImageUrl" style="max-width:100%;" class="img-responsive" alt="">
        </div>

        <div class="entry-content topmargin-min">
            @Html.Raw(Model.Content)
        </div>



        <div class="tags-area clearfix">
            <div class="post-footer  pull-left">
                <a onclick="history.back()" onkeypress="history.back()" class="btn btn-primary">返回</a>
            </div>
        </div>

    </div><!-- post-body end -->
</div>

